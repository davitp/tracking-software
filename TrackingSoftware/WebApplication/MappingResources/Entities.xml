<?xml version="1.0" encoding="utf-8" ?>

<Entities MappingType="BL2DAL" xmlns="http://www.w3schools.com"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xsi:schemaLocation="http://www.w3schools.com EntitiesSchema.xsd">

  <Entity BusinessName="Car" PlainName="car">
    <Property BusinessName="Id" PlainName="id" />
    <Property BusinessName="Color" PlainName="color" />
    <Property BusinessName="Manufacturer" PlainName="manufacturer" />
    <Property BusinessName="Model" PlainName="model" />
  </Entity>

  <Entity BusinessName="User" PlainName="user">
    <Property BusinessName="UserId" PlainName="id" />
    <Property BusinessName="Username"  PlainName="username" />
    <Property BusinessName="Password" PlainName="passwd" />
    <Property BusinessName="FullName" PlainName="fullname" />
  </Entity>

  <Entity BusinessName="CarState" PlainName="carState">
    <Property BusinessName="FuelLevel" PlainName="fuelLevel"  />
    <Property BusinessName="NeedToChangeOil" PlainName="changeOil" />
    <Property BusinessName="When" PlainName="stateTime" />
    <Property BusinessName="Speed" PlainName="speed" />
    <Property BusinessName="Latitude" PlainName="latitude" />
    <Property BusinessName="Longitude" PlainName="longitude" />
  </Entity>
  
</Entities>